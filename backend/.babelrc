{
  "presets": ["@babel/preset-env", "@babel/preset-typescript", "minify"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "constants": "./src/constants",
          "database": "./src/database",
          "models": "./src/models",
          "routes": "./src/routes",
          "controllers": "./src/controllers",
          "responses": "./src/responses",
          "requests": "./src/requests",
          "middlewares": "./src/middlewares"
        }
      }
    ]
  ]
}
